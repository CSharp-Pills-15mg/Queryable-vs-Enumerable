<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net5.0</TargetFramework>
        <RootNamespace>DustInTheWind.QueryableVsEnumerable</RootNamespace>
        <AssemblyName>DustInTheWind.QueryableVsEnumerable</AssemblyName>

        <Company>Dust in the Wind</Company>
        <Authors>Dust in the Wind</Authors>
        <Product>C# Pills 15mg</Product>
        <Copyright>Copyright Â© Dust in the Wind 2021</Copyright>
        <NeutralLanguage>en</NeutralLanguage>

        <Description>A C# Pill that highlights that the Linq query applied after a cast to IEnumerable is not translated into SQL. Instead, it is run in memory.</Description>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.EntityFrameworkCore" Version="5.0.7" />
        <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="5.0.7" />
        <PackageReference Include="Microsoft.EntityFrameworkCore.Tools" Version="5.0.7">
            <PrivateAssets>all</PrivateAssets>
            <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
        </PackageReference>
    </ItemGroup>

</Project>